<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:a="http://schemas.android.com/apk/res/android" a:versionCode="0" a:versionName="0" a:exported="true" 
           xmlns:tools="http://schemas.android.com/tools"
          package='com.example.helloandroid'  >
AddManifestText(
<uses-sdk a:minSdkVersion="31"   a:targetSdkVersion="33"/>
<supports-screens a:largeScreens="true"
    a:normalScreens="true"
    a:smallScreens="true"
    a:anyDensity="true"/>)
        

        <application a:label='A Hello Android'    >
            
         
            
          <activity  a:exported='true' a:name='com.example.helloandroid.MainActivity'   >
              
             <intent-filter>
                 <action a:name='android.intent.action.MAIN'/>
                 <category a:name='android.intent.category.LAUNCHER'/>
                 
             </intent-filter>
         
           </activity>
        
            
           <activity
            android:name="com.yalantis.ucrop.UCropActivity"
            android:exported="false"
            tools:node="merge"
            tools:overrideLibrary="com.reactnative.ivpusic.imagepicker"
           />
            
            
        </application>
</manifest>
