<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:a="http://schemas.android.com/apk/res/android" a:versionCode="0" a:versionName="0" a:exported="true" package='com.example.helloandroid'  >
AddManifestText(
<uses-sdk a:minSdkVersion="30"   a:targetSdkVersion="34"/>
<supports-screens a:largeScreens="true"
    a:normalScreens="true"
    a:smallScreens="true"
    a:anyDensity="true"/>)
        

        <application a:label='A Hello Android'    >
            
            
            
          <activity 
             a:name='com.example.helloandroid.MainActivity'  a:exported="true"  tools:node="merge"  >
              
             <intent-filter>
                 <action a:name='android.intent.action.MAIN'/>
                 <category a:name='android.intent.category.LAUNCHER'/>
                 
             </intent-filter>
          </activity>
          
        </application>
</manifest>
